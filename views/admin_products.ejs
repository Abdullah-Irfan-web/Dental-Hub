    <div class="container my-5">
        <h2 class="mb-4 text-center">All Products</h2>

        <form class="form-inline mb-4 d-flex justify-content-center" method="GET" action="/admin/products">
            <input type="text" name="search" class="form-control mr-2" placeholder="Search by Name or Order ID" value="<%= search %>">
            <button type="submit" class="btn btn-primary">Search</button>
          </form>
        <div class="table-responsive">
          <table class="table table-bordered table-hover">
            <thead class="thead-dark">
              <tr>
                <th>Image</th>
                <th>Name</th>
                <th>Description</th>
                <th>Price</th>
                <th>Stock</th>
                <th>Category</th>
                <th>Actions</th>
              </tr>
            </thead>
            <tbody>
              <% products.forEach(product => { %>
                <tr>
                  <td><img src="/img/<%= product.img %>" alt="<%= product.productName %>" width="60"></td>
                  <td><%= product.productName %></td>
                  <td><%= product.shortDescription %></td>
                  <td>â‚¹<%= product.price %></td>
                  <td><%=product.totalStock%></td>
                  <td><%=product.category%></td>
                  
                  <td>
                    <a href="/admin/products/edit/<%= product._id %>" class="btn btn-sm btn-warning">Edit</a>
                    <a href="/admin/products/delete/<%= product._id %>" class="btn btn-sm btn-danger" onclick="return confirm('Delete this product?')">Delete</a>
                  </td>
                </tr>
              <% }) %>
            </tbody>
          </table>
        </div>

        <nav aria-label="Orders pagination">
            <ul class="pagination justify-content-center">
              <% if (currentPage > 1) { %>
                <li class="page-item"><a class="page-link" href="/admin?page=<%= currentPage - 1 %>&search=<%= search %>">Prev</a></li>
              <% } %>
              
              <% for(let i = 1; i <= totalPages; i++) { %>
                <li class="page-item <%= currentPage === i ? 'active' : '' %>">
                  <a class="page-link" href="/admin?page=<%= i %>&search=<%= search %>"><%= i %></a>
                </li>
              <% } %>
              
              <% if (currentPage < totalPages) { %>
                <li class="page-item"><a class="page-link" href="/admin/orders?page=<%= currentPage + 1 %>&search=<%= search %>">Next</a></li>
              <% } %>
            </ul>
          </nav>
      </div>
      








